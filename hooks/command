#!/bin/bash
set -ueox pipefail

env

id

which docker

cat ~/.docker/config.json

docker images

ls -l /namespace/bin/

# docker-credential-nsc

ls -l /var/run/nsc

ls -l $(which docker)

docker version

docker pull nscr.io/01gr1g2rpb7ahzddy3f227exq9/test-cache:v1

docker compose -f .buildkite/docker/docker-compose.yml pull
